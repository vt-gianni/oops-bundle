{
    "name": "vtgianni/oops-bundle",
    "description": "A Symfony bundle to log API requests errors.",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Gianni GIUDICE",
            "email": "giannigiudice.fr@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": ">=5.4",
        "doctrine/orm": ">=2.13"
    },
    "autoload": {
        "psr-4": {
            "VTGianni\\OopsBundle\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": ">=5.4"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
